{
  "name": "rcm-titan-asi",
  "version": "7.4.0-STABLE-GODMODE",
  "description": "Artificial Super Intelligence Core | Pure JS | Hyper-Scale Optimized",
  "main": "server.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "start:godmode": "cross-env NODE_ENV=production pm2 start server.js -i max --name titan-core --node-args='--max-old-space-size=8192 --expose-gc --use-largepages'",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "worker": "cross-env NODE_ENV=production node workers/ai-processor.js",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "clean": "echo 'Manual delete node_modules required on Windows'",
    "reinstall": "npm install",
    "monitor": "pm2 monit",
    "test": "jest --detectOpenHandles --forceExit --coverage",
    "docker:build": "docker build -t rcm-titan-asi:stable ."
  },
  "dependencies": {
